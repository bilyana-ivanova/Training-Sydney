    "Condition": "Telerik.Sitefinity.DynamicTypes.Model.Issues.Issue AND MVC",
    "NameForDevelopers": "Detail-Issue",
    "Id": "06930032-ebfb-67f3-ba7d-ff000072e94b",
    "Name": "Detail.Issue",
    "Theme": null,
    "DataType": "ASP_NET_TEMPLATE",
    "Data": "@model Telerik.Sitefinity.Frontend.Mvc.Models.ContentDetailsViewModel\r\n\r\n@using System.Collections;\r\n@using System.Linq;\r\n@using Telerik.Sitefinity.Frontend.DynamicContent.TemplateGeneration.Fields.Helpers;\r\n@using Telerik.Sitefinity;\r\n@using Telerik.Sitefinity.Data.ContentLinks;\r\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\r\n@using Telerik.Sitefinity.Libraries.Model;\r\n@using Telerik.Sitefinity.Localization;\r\n@using Telerik.Sitefinity.Model;\r\n@using Telerik.Sitefinity.Model.ContentLinks;\r\n@using Telerik.Sitefinity.Modules.Pages;\r\n@using Telerik.Sitefinity.Pages.Model; \r\n\r\n<div class=\"@Model.CssClass\" @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)Model.Item.Fields.Id)>\r\n    <h3 @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>\r\n        @Model.Item.Fields.Title\r\n    </h3>\r\n    <div class=\"text-muted\">\r\n        @Model.Item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\r\n    </div>\r\n    \r\n    \r\n@*Start Description field*@\r\n<div @Html.InlineEditingFieldAttributes(\"Description\", \"LongText\")>@Html.Raw(Model.Item.Fields.Description)</div>\r\n@*End Description field*@\r\n\r\n@*Start Title field*@\r\n<div>\r\n    <strong> Title :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\")>@Model.Item.Fields.Title</span>    \r\n</div>\r\n@*End Title field*@\r\n\r\n@*Start IssueNumber field*@\r\n<div>\r\n    <strong> Issue number :</strong>\r\n\r\n    <span @Html.InlineEditingFieldAttributes(\"IssueNumber\", \"ShortText\")>@Model.Item.Fields.IssueNumber</span>    \r\n</div>\r\n@*End IssueNumber field*@\r\n\r\n@*Start IssueCover field*@\r\n@if(Model.Item.Fields.IssueCover != null)\r\n{\r\n    <img src='@Model.Item.Fields.IssueCover.Fields.MediaUrl' alt='@Model.Item.Fields.IssueCover.Fields.AlternativeText' title='@Model.Item.Fields.IssueCover.Fields.Title' />\r\n}\r\n@*End IssueCover field*@\r\n\r\n@*Start IssueDocument field*@\r\n@if(Model.Item.Fields.IssueDocument != null)\r\n{\r\n    <a href='@Model.Item.Fields.IssueDocument.Fields.MediaUrl'>@Model.Item.Fields.IssueDocument.Fields.Title</a>\r\n}\r\n@*End IssueDocument field*@\r\n\r\n@*Start FeaturedArticle field*@\r\n<div>\r\n    <strong> Related news :</strong>\r\n    \r\n    @if(Model.Item.Fields.FeaturedArticle != null)\r\n    {\r\n        <a href='@Model.Item.Fields.FeaturedArticle.DefaultUrl'>\r\n            @Model.Item.Fields.FeaturedArticle.Identifier\r\n        </a>\r\n    }\r\n</div>\r\n@*End FeaturedArticle field*@\r\n\r\n@*Start Articles field*@\r\n<div>\r\n    <strong> Related news</strong>\r\n\r\n    <ul>\r\n    @foreach (var relatedItem in Model.Item.Fields.Articles)\r\n    {\r\n        <li>\r\n            <a href='@relatedItem.DefaultUrl'>\r\n                @relatedItem.Identifier\r\n            </a>\r\n        </li>   \r\n    }\r\n    </ul>\r\n</div>\r\n@*End Articles field*@\r\n\r\n    \r\n    @if (Model.EnableSocialSharing)\r\n    {\r\n        @Html.SocialShareOptions()\r\n    }\r\n</div>",
    "DateCreated": "2014-12-08T16:08:35.12Z",
    "ResourceAssemblyName": "Telerik.Sitefinity.Resources",
