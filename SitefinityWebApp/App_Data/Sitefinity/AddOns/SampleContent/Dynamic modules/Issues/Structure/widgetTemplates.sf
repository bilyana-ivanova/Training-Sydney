  {
    "ControlType": "Telerik.Sitefinity.Frontend.DynamicContent.Mvc.Controllers.DynamicContentController",
    "FriendlyControlName": "Issues - Issues - list (MVC)",
    "IsDifferentFromEmbedded": true,
    "ResourceClassId": null,
    "AreaName": "Issues - Issue",
    "Condition": "Telerik.Sitefinity.DynamicTypes.Model.Issues.Issue AND MVC",
    "NameForDevelopers": "List-Issue",
    "Id": "04930032-ebfb-67f3-ba7d-ff000072e94b",
    "Name": "List.Issue",
    "Theme": null,
    "DataType": "ASP_NET_TEMPLATE",
    "Data": "@model Telerik.Sitefinity.Frontend.DynamicContent.Mvc.Models.DynamicContentListViewModel\n\n@using Telerik.Sitefinity.Frontend.DynamicContent.TemplateGeneration.Fields.Helpers;\n@using Telerik.Sitefinity;\n@using Telerik.Sitefinity.Data.ContentLinks;\n@using Telerik.Sitefinity.Frontend.Mvc.Helpers;\n@using Telerik.Sitefinity.Frontend.Mvc.Models;\n@using Telerik.Sitefinity.Web.DataResolving;\n@using Telerik.Sitefinity.Model.ContentLinks;\n@using Telerik.Sitefinity.Modules.Pages;\n\n@Html.Script(ScriptRef.JQuery)\n\n<link rel=\"stylesheet\" href='@Url.WidgetContent(\"//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css\")' type=\"text/css\" />\n<script src=\"//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js\"></script>\n\n<div class=\"@Model.CssClass\">\n    <ul class=\"list-unstyled\">\n        @foreach (var item in Model.Items)\n        {\n            <li @Html.InlineEditingAttributes(Model.ProviderName, Model.ContentType.FullName, (Guid)item.Fields.Id)>\n                \n\n                <h3>\n                    @if (ViewBag.OpenInSamePage)\n                    {\n                        <a @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\") href=\"@(ViewBag.CurrentPageUrl + item.Fields.ItemDefaultUrl)\">\n                            @item.Fields.Title\n                        </a>\n                    }\n                    else\n                    {\n                        <a @Html.InlineEditingFieldAttributes(\"Title\", \"ShortText\") href=\"@HyperLinkHelpers.GetDetailPageUrl(item.DataItem, ViewBag.DetailsPageId)\">\n                            @item.Fields.Title\n                        </a>\n                    }\n                </h3>\n                <div class=\"text-muted\">\n                    @item.GetDateTime(\"PublicationDate\", \"MMM d, yyyy, HH:mm tt\")\n                </div>\n            </li>\n        }\n    </ul>\n\n    @if (Model.ShowPager)\n    {\n        @Html.Action(\"Index\", \"ContentPager\", new { currentPage = Model.CurrentPage, \n                        totalPagesCount = Model.TotalPagesCount.Value, \n                        redirectUrlTemplate = ViewBag.RedirectPageUrlTemplate })\n    }\n\n</div>",
